{
  "compilerOptions": {
    "module": "es6",
    "outDir": "./dist/",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noImplicitAny": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
